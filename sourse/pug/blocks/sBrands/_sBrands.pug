mixin sBrands(data)
	// start sBrands
	+b.SECTION.sBrands.section#sBrands&attributes(attributes)
		.container
			+e.row.row
				mixin item(img)
					.col-6.col-md-4.col-lg-3
						+e.item(href='#')
							.img-wrap-center
								+img(`img/${img}`)
				+item('sOurBrands-img-1.jpg')
				+item('sBrands-img-1.png')
				+item('sBrands-img-2.png')
				+item('sBrands-img-3.png')
